{
  "name": "wolfling/silex-blog",
  "description": "silex-blog",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "wolfling",
      "email": "wolfling@i.ua"
    }
  ],
  "autoload": {
    "psr-4": {
      "": "src"
    },
    "files": [
      "app/bootstrap.php",
      "app/services.php",
      "app/middlewares.php",
      "app/exceptions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests"
    }
  },
  "require": {
    "silex/silex": "^2.1",
    "symfony/config": "^3.3",
    "symfony/yaml": "^3.3",
    "simple-bus/message-bus": "^2.2",
    "incenteev/composer-parameter-handler": "^2.1",
    "symfony/console": "^3.3",
    "doctrine/dbal": "^2.5",
    "doctrine/migrations": "^1.5",
    "symfony/validator": "^3.3",
    "symfony/finder": "^3.3",
    "jms/serializer": "^1.7",
    "symfony/property-access": "^3.3",
    "dflydev/doctrine-orm-service-provider": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "symfony/browser-kit": "^3.3"
  },
  "scripts": {
    "bootstrap-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    ],
    "post-install-cmd": [
      "@bootstrap-scripts"
    ],
    "post-update-cmd": [
      "@bootstrap-scripts"
    ]
  },
  "extra": {
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
